{
  "name": "aegis-pro-vue-pages",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --base /",
    "dev:address": "vite serve src/pages/address",
    "dev:person": "vite serve src/pages/person",
    "build:address": "cross-env PAGENAME=address vite build",
    "build:person": "cross-env PAGENAME=person vite build",
    "format": "prettier --write \"src/**/*.{js,ts,jsx,tsx,vue,css,scss,html,htm,json}\""
  },
  "dependencies": {
    "sweetalert": "1.1.3",
    "vue": "3.2.47"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "4.1.0",
    "@vitejs/plugin-vue-jsx": "3.0.1",
    "cross-env": "7.0.3",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-only-warn": "1.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.8.8",
    "sass": "^1.64.1",
    "vite": "4.3.9"
  }
}
